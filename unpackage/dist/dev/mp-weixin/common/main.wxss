/*每个页面公共css */
.flex_between {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
}
.clear {
	clear: both;
	content: '';
	display: block;
	height: 0;
}

